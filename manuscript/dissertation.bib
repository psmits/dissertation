number = {1646},
pages = {2031--2037},
pmid = {18508747},
title = {{A common tendency for phylogenetic overdispersion in mammalian assemblages}},
url = {http://rspb.royalsocietypublishing.org/cgi/doi/10.1098/rspb.2008.0420{\%}5Cnhttp://rspb.royalsocietypublishing.org/content/275/1646/2031.abstract{\%}5Cnfile://c/Documents and Settings/Cristina/Meus documentos/My Dropbox/Meu Documentos/Papers/2008/Cooper etal 2008 P},
