@article{Foote2012b,
abstract = {The distribution of species among genera and higher taxa has largely untapped potential to reveal among-clade variation in rates of origination and extinction. The probability distribution of the number of species within a genus is modelled with a stochastic, time-homogeneous birth-death model having two parameters: the rate of species extinction, $\mu$, and the rate of genus origination, $\gamma$, each scaled as a multiple of the rate of within-genus speciation, $\lambda$. The distribution is more sensitive to $\gamma$ than to $\mu$, although $\mu$ affects the size of the largest genera. The species : genus ratio depends strongly on both $\gamma$ and $\mu$, and so is not a good diagnostic of evolutionary dynamics. The proportion of monotypic genera, however, depends mainly on $\gamma$, and so may provide an index of the genus origination rate. Application to living marine molluscs of New Zealand shows that bivalves have a higher relative rate of genus origination than gastropods. This is supported by the analysis of palaeontological data. This concordance suggests that analysis of living taxonomic distributions may allow inference of macroevolutionary dynamics even without a fossil record.},
author = {Foote, Michael},
