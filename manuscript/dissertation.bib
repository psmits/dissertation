journal = {Evolution},
keywords = {Adaptation,Animals,Biological Evolution,Body Size,Cuba,Dominican Republic,Geography,Jamaica,Likelihood Functions,Lizards,Lizards: anatomy {\&} histology,Phenotype,Physiological,Puerto Rico},
month = {sep},
