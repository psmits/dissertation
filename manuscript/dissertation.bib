issn = {0962-8452},
journal = {Proceedings. Biological sciences / The Royal Society},
keywords = {Animals,Conservation of Natural Resources,Extinction, Biological,Humans,Mammals,Mammals: physiology,Models, Biological,Risk Assessment},
month = {jun},
number = {1641},
pages = {1441--8},
pmid = {18367443},
title = {{The predictability of extinction: biological and external correlates of decline in mammals.}},
