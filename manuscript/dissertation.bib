abstract = {Tarka and Tarkadectes are Middle Eocene mammals known only from the Rocky Mountains region of North America. Previous work has suggested that they are members of the Plagiomenidae, an extinct family often included in the order Dermoptera. Here we describe a new primate, Tarkops mckennai gen. et sp. nov., from the early Middle Eocene Irdinmanha Formation of Inner Mongolia, China. The new taxon is particularly similar to Tarka and Tarkadectes, but it also displays many features observed in omomyids. A phylogenetic analysis based on a data matrix including 59 taxa and 444 dental characters suggests that Tarkops, Tarka and Tarkadectes form a monophyletic group--the Tarkadectinae--that is nested within the omomyid clade. Within Omomyidae, tarkadectines appear to be closely related to Macrotarsius. Dermoptera, including extant and extinct flying lemurs and plagiomenids, is recognized as a clade nesting within the polyphyletic group of plesiadapiforms, therefore supporting the previous suggestion that the relationship between dermopterans and primates is as close as that between plesiadapiforms and primates. The distribution of tarkadectine primates on both sides of the Pacific Ocean basin suggests that palaeoenvironmental conditions appropriate to sustain primates occurred across a vast expanse of Asia and North America during the Middle Eocene.},
author = {Ni, Xijun and Meng, Jin and Beard, K Christopher and Gebo, Daniel L and Wang, Yuanqing and Li, Chuankui},
doi = {10.1098/rspb.2009.0173},
file = {:home/peter/Documents/Mendeley Desktop/Ni et al/Ni et al. - 2010 - Proceedings. Biological sciences The Royal Society.pdf:pdf},
issn = {1471-2954},
journal = {Proceedings. Biological sciences / The Royal Society},
keywords = {Animals,China,Dentition,Fossils,Geography,Jaw,Jaw: anatomy {\&} histology,Phylogeny,Primates,Primates: anatomy {\&} histology,Primates: classification,Tooth,Tooth: anatomy {\&} histology},
month = {jan},
number = {1679},
pages = {247--56},
pmid = {19386655},
title = {{A new tarkadectine primate from the Eocene of Inner Mongolia, China: phylogenetic and biogeographic implications.}},
