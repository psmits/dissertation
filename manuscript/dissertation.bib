keywords = {Age Factors,Animals,Ceratitis capitata,Ceratitis capitata: growth {\&} development,Ceratitis capitata: metabolism,Ceratitis capitata: physiology,Computational Biology,Computational Biology: methods,Energy Metabolism,Environment,Models, Biological,Mortality,Population Growth,Reproduction},
month = {jan},
