keywords = {Animals,Biodiversity,Biological Evolution,Conservation of Natural Resources,Geography,Mammals,Mammals: anatomy {\&} histology,Mammals: genetics,Mammals: physiology,Phenotype,Phylogeny},
month = {dec},
number = {1749},
pages = {4997--5003},
pmid = {23097512},
title = {{Traits, trees and taxa: global dimensions of biodiversity in mammals.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=3497241{\&}tool=pmcentrez{\&}rendertype=abstract},
