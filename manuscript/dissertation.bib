journal = {Palaios},
keywords = {Adaptation, Biological,Animals,Biological Evolution,Fossils,Invertebrates,Invertebrates: classification,Marine Biology,Models, Biological,Plants,Plants: classification,Vertebrates,Vertebrates: classification},
month = {jan},
