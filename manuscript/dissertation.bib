isbn = {1461-023X (Print)$\backslash$n1461-023X (Linking)},
issn = {1461023X},
journal = {Ecology Letters},
keywords = {Body size,Dispersal,Environmental heterogeneity,Mass effects,Meta-analysis,Metacommunity theory,Neutral theory,Patch dynamics,Species sorting,Variation decomposition},
number = {11},
pages = {1175--1182},
pmid = {21352441},
title = {{Integrating environmental and spatial processes in ecological community dynamics}},
volume = {8},
